<% include partials/head.ejs %>
<body>
<h1>Todo List</h1>
    <div id="todo-table">
        <form>
            <input type="text" name="item" placeholder="Add new item..." required>
            <button type="submit">Add Item</button>
        </form>
        <ul>
        <% for (let i=0; i < todos.length; i++){%>
            <li><%= todos[i].item %></li>
        <%}%>
        </ul>
    </div>
</body>
</html>